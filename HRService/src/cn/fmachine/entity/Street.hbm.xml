<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.fmachine.entity.Street" table="STREET">
        <id name="id" type="java.lang.Integer">
            <column name="ID" precision="9" scale="0" />
            <generator class="sequence" >
            	<param name="sequence">STREET_ID</param>
            </generator>
        </id>
        <property name="name" type="java.lang.String">
            <column name="NAME" length="50" />
        </property>
        <many-to-one name="district" class="cn.fmachine.entity.District" fetch="select">
            <column name="DISTRICT_ID" precision="9" scale="0" />
        </many-to-one>
    </class>
</hibernate-mapping>

<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.fmachine.entity.HouseUser" table="HOUSE_USER">
        <id name="id" type="java.lang.Integer">
            <column name="ID" precision="9" scale="0" />
            <generator class="sequence" >
            	<param name="sequence">HOUSE_USER_ID</param>
            </generator>
        </id>
        <property name="password" type="java.lang.String">
            <column name="PASSWORD" length="50" />
        </property>
        <property name="telephone" type="java.lang.String">
            <column name="TELEPHONE" length="15" />
        </property>
        <property name="username" type="java.lang.String">
            <column name="USERNAME" length="50" />
        </property>
        <property name="isadmin" type="java.lang.String">
            <column name="ISADMIN" length="5" />
        </property>
        <property name="realname" type="java.lang.String">
            <column name="REALNAME" length="50" />
        </property>
    </class>
</hibernate-mapping>
